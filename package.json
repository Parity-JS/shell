{
  "name": "shell",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/api": {
      "target": "http://127.0.0.1:8180",
      "changeOrigin": true,
      "autoRewrite": true
    },
    "/app": {
      "target": "http://127.0.0.1:8545",
      "changeOrigin": true,
      "pathRewrite": {
        "^/app": ""
      }
    },
    "/parity-utils": {
      "target": "http://127.0.0.1:3000",
      "changeOrigin": true,
      "pathRewrite": {
        "^/parity-utils": ""
      }
    },
    "/rpc": {
      "target": "http://127.0.0.1:8545",
      "changeOrigin": true
    }
  },
  "scripts": {
    "electron": "electron src/index.electron.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@parity/api": "^2.1.16",
    "@parity/shared": "^2.2.21",
    "@parity/ui": "^3.0.22",
    "lodash": "^4.17.4",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-intl": "^2.4.0",
    "react-scripts": "1.1.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.77.2",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "electron": "^1.7.10",
    "parity-react-scripts": "github:paritytech/parity-react-scripts"
  }
}
