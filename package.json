{
  "name": "shell",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "proxy": {
    "/api": {
      "target": "http://127.0.0.1:8180",
      "changeOrigin": true,
      "autoRewrite": true
    },
    "/app": {
      "target": "http://127.0.0.1:8545",
      "changeOrigin": true,
      "pathRewrite": {
        "^/app": ""
      }
    },
    "/parity-utils": {
      "target": "http://127.0.0.1:3000",
      "changeOrigin": true,
      "pathRewrite": {
        "^/parity-utils": ""
      }
    },
    "/rpc": {
      "target": "http://127.0.0.1:8545",
      "changeOrigin": true
    }
  },
  "scripts": {
    "build": "react-app-rewired build && yarn run build:inject && yarn run electron",
    "build:inject": "webpack --config webpack.inject.config.js",
    "electron": "electron src/index.electron.js",
    "electron:dev": "yarn run electron --dev",
    "start": "yarn run build:inject && react-app-rewired start",
    "test": "react-app-rewired test --env=jsdom"
  },
  "dependencies": {
    "@parity/api": "^2.1.19",
    "@parity/mobx": "^1.0.7",
    "@parity/plugin-signer-account": "parity-js/plugin-signer-account#c1272caa242c8b97dac78e5d0b1e068614657fdc",
    "@parity/shared": "^2.2.21",
    "@parity/ui": "^3.0.23",
    "lodash": "^4.17.4",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-intl": "^2.4.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.77.2",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "electron": "^1.7.10",
    "react-app-rewire-mobx": "^1.0.7",
    "react-app-rewired": "^1.4.0"
  }
}
